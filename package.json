    "dist:macos": "electron-builder build --x64 --macos"
  },
  "license": "MIT",
  "dependencies": {
    "swagger-ui-dist": "^4.6.2"
  },
  "devDependencies": {
    "app-builder-bin-fixer": "4.1.0",
    "electron-builder": "24.13.3",
    "electron": "30.0.2"
  }
}
